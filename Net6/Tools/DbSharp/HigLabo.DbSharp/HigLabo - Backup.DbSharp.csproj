    <Authors>Higty</Authors>
    <Copyright>Copyright Â© 2011- HigLabo All Rights Reserved</Copyright>
    <RepositoryUrl>https://github.com/higty/higlabo</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Core\DbParameterExtensions.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="4.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="HigLabo.CodeGenerator">
      <HintPath>..\Reference\HigLabo.CodeGenerator.dll</HintPath>
    </Reference>
    <Reference Include="HigLabo.Core">
      <HintPath>..\Reference\HigLabo.Core.dll</HintPath>
    </Reference>
    <Reference Include="HigLabo.Data">
      <HintPath>..\Reference\HigLabo.Data.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
