    <ProjectReference Include="..\HigLabo.Mapper\HigLabo.Mapper.csproj" />
    <ProjectReference Include="..\HigLabo.Net.OAuth\HigLabo.Net.OAuth.csproj" />
  </ItemGroup>

</Project>
